{
  "name": "uruguay-phone-validator",
  "version": "1.0.0",
  "description": "Express server with TypeScript to validate Uruguayan phone numbers",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:simple": "ts-node src/index.ts",
    "dev:test": "ts-node test-server.ts",
    "vercel-build": "echo 'Vercel handles TypeScript compilation for serverless functions'",
    "test": "ts-node test-comprehensive.ts",
    "test:phone": "ts-node test-phone-checker.ts",
    "test:json": "ts-node test-json-simple.ts",
    "test:enhanced": "ts-node test-enhanced-features.ts",
    "test:api": "bash test_api.sh",
    "test:api:complete": "bash test-api-complete.sh",
    "test:api:node": "ts-node test-api.ts",
    "test:vercel": "npm run test:api:node https://no-llamar-uy.vercel.app"
  },
  "keywords": [
    "express",
    "typescript",
    "phone-validation",
    "uruguay"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.1.0",
    "express": "^4.18.2",
    "google-libphonenumber": "^3.2.34",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/express": "^4.17.21",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/node": "^20.10.0",
    "@types/tesseract.js": "^0.0.2",
    "@vercel/node": "^5.3.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0",
    "vercel": "^44.5.0"
  }
}
